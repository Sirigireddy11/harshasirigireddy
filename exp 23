#include<stdio.h>
#include<stdlib.h>
int main()
{
int n,i,head,seek=0;
int req[50];
printf("Enter number of requests:");
scanf("%d",&n);
printf("Enter request sequence:");
for(i=0;i<n;i++)
scanf("%d",&req[i]);
printf("Enter initial head position:");
scanf("%d",&head);
for(i=0;i<n;i++)
{
seek+=abs(req[i]-head);
head=req[i];
}
printf("Total seek time:%d",seek);
return 0;
}
